{
  "name": "giggity",
  "description": "Giggity Backend",
  "version": "0.0.0",
  "homepage": "",
  "main": "./backend",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Greg Michalec",
    "email": "greg@primate.net"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "./backend",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "yarn": ">= 0.18.0"
  },
  "scripts": {
    "start-client": "PORT=4000 BROWSER=none react-scripts start",
    "start-server": "nodemon ./backend/",
    "start": "concurrently -k \"yarn run start-server\" \"yarn run start-client\"",
    "test-backend": "yarn run eslint && NODE_ENV= yarn run mocha",
    "eslint": "eslint ./backend/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "build": "react-scripts build",
    "start:prod": "PORT=8000 node backend/index.js",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@feathersjs/authentication": "^2.1.13",
    "@feathersjs/authentication-client": "^1.0.11",
    "@feathersjs/authentication-jwt": "^2.0.7",
    "@feathersjs/authentication-local": "^1.2.7",
    "@feathersjs/authentication-oauth2": "^1.2.5",
    "@feathersjs/client": "^3.7.7",
    "@feathersjs/configuration": "^2.0.4",
    "@feathersjs/errors": "^3.3.4",
    "@feathersjs/express": "^1.2.7",
    "@feathersjs/feathers": "^3.2.3",
    "@feathersjs/socketio": "^3.2.7",
    "@feathersjs/socketio-client": "^1.2.1",
    "@material-ui/core": "^3.5.1",
    "@material-ui/icons": "^3.0.2",
