  "devDependencies": {
    "@feathersjs/cli": "^3.9.0",
    "concurrently": "^4.1.0",
    "create-react-app": "^2.1.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.7",
    "request": "^2.88.0",
    "request-promise": "^4.2.2"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3030",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
