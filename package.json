    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "feathers-mongoose": "^7.2.0",
    "formik": "^1.3.2",
    "formik-material-ui": "^0.0.16",
    "helmet": "^3.15.0",
    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "mongodb-core": "^3.1.9",
    "mongoose": "^5.4.3",
    "passport-auth0": "^1.1.0",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
